{
  "rewrites": [
    {
      "source": "/",
      "destination": "/src/pages/login.html"
    },
    {
      "source": "/login",
      "destination": "/src/pages/login.html"
    },
    {
      "source": "/painel",
      "destination": "/src/pages/painel.html"
    },
    {
      "source": "/registros",
      "destination": "/src/pages/registros.html"
    },
    {
      "source": "/troca-oleo",
      "destination": "/src/pages/trocaOleo.html"
    },
    {
      "source": "/trocas-registradas",
      "destination": "/src/pages/trocasRegistradas.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
